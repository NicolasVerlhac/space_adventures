<%= raise %>
<h1>Vehicules#edit</h1>
<p>Find me in app/views/vehicules/edit.html.erb</p>

<% @vehicules.each do |vehicule| %>
  <% if policy(vehicule).edit? %>
    <%= link_to "Update", edit_vehicule_path(vehicule) %>
  <% end %>
<% end %>

<%= form_for(@vehicule) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :completed %>
  <%= f.text_field :completed %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :capacity %>
  <%= f.text_field :capacity %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :daily_price %>
  <%= f.text_field :daily_price %>

  <%= f.label :daily_price %>
  <%= f.text_field :daily_price %>

  <%= f.label :availability %>
  <%= f.text_field :availability %>

<%= f.submit 'Edit' %>
<% end %>


